{
  "expo": {
    "name": "Paynest",
    "slug": "Paynest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.jpg",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#246BFD"
      },
      "package": "com.hassan2434.AllPay"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2efc11f5-51b3-4d24-a2ad-97a38d51a2a3"
      }
    }
  }
}
